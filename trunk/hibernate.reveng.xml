<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

	<table name="account" catalog="bank" class="Account"></table>
	<table name="custloginfo" catalog="bank" class="CustLogInfo">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="customer" catalog="bank" class="Customer">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="enterpriseacc" catalog="bank" class="EnterpriseAcc">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="optrecord" catalog="bank" class="OptRecord">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="regularacc" catalog="bank" class="RegularAcc">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="vipacc" catalog="bank" class="VIPAcc">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>